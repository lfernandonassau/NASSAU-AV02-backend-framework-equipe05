{
    "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "jsx": "react-jsx",
    "moduleResolution": "bundler",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "baseUrl": "src",
    "paths": {
        "components/*": ["components/*"],
        "pages/*": ["pages/*"],
        "context/*": ["context/*"],
        "services/*": ["services/*"],
        "types/*": ["types/*"]
    }
    },
    "include": ["src", "src/services"]
}
