{
  "name": "kodan-monorepo",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "concurrently \"yarn workspace frontend dev\" \"yarn workspace backend dev\"",
    "dev:front": "yarn workspace frontend dev",
    "dev:back": "yarn workspace backend dev",
    "postinstall": "yarn workspace backend prisma:generate"
  },
  "devDependencies": {
    "concurrently": "^9.0.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
